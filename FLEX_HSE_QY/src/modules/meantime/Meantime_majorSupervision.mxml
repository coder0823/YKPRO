//				column = new AdvancedDataGridColumn();
//				column.dataField = "corpName";
//				column.headerText = "企业名称";
//				column.itemRenderer = meantimeOneStatusRenderer_corpName;
//				column.width = 100;
//				groupColumn.push(column);
				
				column = new AdvancedDataGridColumn();
