							</s:VGroup>
						</s:BorderContainer>
					</s:HGroup>
					<s:HGroup width="100%" height="50%" horizontalAlign="center">
						<s:BorderContainer width="50%" height="99%" styleName="contentBox">
							<s:VGroup width="100%" height="100%" horizontalAlign="center" verticalAlign="top">
								<views:BarSubTotalMulti showZoomControlBox="true" ShowDataLabel="{showDataLabel}" width="100%" height="100%" showTitle="true" showLegend="true" xAxisAt="bottom" yAxisAt="left"/>
